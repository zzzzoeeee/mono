		"@anatine/zod-openapi": "^2.2.8",
		"@repo/typescript-config": "workspace:*",
		"@ts-rest/core": "^3.52.1",
		"@ts-rest/open-api": "^3.52.1",
		"@types/express": "^5.0.0",
		"@types/swagger-ui-express": "^4.1.8",
		"express": "^5.1.0",
		"swagger-ui-express": "^5.0.1",
		"zod": "^3.25.30"
